{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Listen for XDebug on Docker",
          "type": "php",
          "request": "launch",
          "port": 9003,
          "pathMappings": {
              "/var/www/html/": "${workspaceFolder}/project"
          },
          "xdebugSettings": {
            "show_hidden": 1,
            "max_children": 999,
          }
      }
  ]
}